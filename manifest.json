{
  "manifest_version": 3,
  "name": "DC Management Console",
  "version": "1.4",
  "description": "Console for Chrome Management",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": { "48": "icons/icon48.png", "128": "icons/icon128.png" },
  "update_url": "https://cdn.jsdelivr.net/gh/pnut8/DC-Console@master/updates.xml"
}
